    <script>
        function verifyInput(form) {
            if (form.username.value.length == 0) {
                alert("請輸入姓名");
                form.username.focus();
                form.username.select();
                return false;
            }
            else if (form.tel.value.length == 0) {
                alert("請輸入電話!");
                form.tel.focus();
                form.tel.select();
                return false;
            }
            else if (form.email.value.length == 0) {
                alert("請輸入郵箱");
                form.email.focus();
                form.email.select();
                return false;
            }
            return true;
        }

    </script>
